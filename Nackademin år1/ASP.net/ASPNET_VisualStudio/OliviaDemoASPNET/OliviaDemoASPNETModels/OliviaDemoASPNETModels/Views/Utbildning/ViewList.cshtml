@model List<OliviaDemoASPNETModels.Models.Utbildning>

@foreach(var utbildning in Model)
{
    <tr>
        <td><h1>@utbildning.Namn</h1></td>
    </tr>
    <tr>
        <td><p>@utbildning.Langd år</p></td>
    </tr>
    <tr>
        <td><p>@utbildning.Stad</p></td>
    </tr>
    <tr>
        <td>
            <a asp-controller="Utbildning" asp-action="ViewDetails">Läs mer om utbildningen</a>
        </td>
    </tr>
    <br /> <br />
}